{
  "version": 2,
  "routes": [
    {
      "src": "/(.*)",
      "headers": {
        "host": "szamotuly\\.example\\.com"
      },
      "dest": "/szamotuly.html"
    },
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/szamotuly",
      "dest": "/szamotuly.html"
    },
    {
      "src": "/fonts/(.*)",
      "dest": "/fonts/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/styles/(.*)",
      "dest": "/styles/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000"
      }
    },
    {
      "src": "/img/(.*)",
      "dest": "/img/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000"
      }
    },
    {
      "src": "/scripts/(.*)",
      "dest": "/scripts/$1",
      "headers": {
        "Cache-Control": "public, max-age=86400"
      }
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}